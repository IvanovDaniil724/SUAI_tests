<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:id="@+id/itemTableBorderBottom"
        android:left="-2dp"
        android:right="-2dp"
        android:top="-2dp">
        <shape android:id="@+id/itemTableBorderBottomShape" android:shape="rectangle">
            <stroke
                android:id="@+id/itemTableBorderBottomStroke"
                android:width="2dp"
                android:color="@color/suai_secondary"/>
        </shape>
    </item>

    <item
        android:id="@+id/itemTableBorderTop"
        android:left="-2dp"
        android:right="-2dp"
        android:bottom="-2dp">
        <shape android:id="@+id/itemTableBorderTopShape" android:shape="rectangle">
            <stroke
                android:id="@+id/itemTableBorderTopStroke"
                android:width="2dp"
                android:color="@color/suai_secondary"/>
        </shape>
    </item>

    <item
        android:id="@+id/itemTableBorderLeft"
        android:bottom="-2dp"
        android:right="-2dp"
        android:top="-2dp">
        <shape android:id="@+id/itemTableBorderLeftShape" android:shape="rectangle">
            <stroke
                android:id="@+id/itemTableBorderLeftStroke"
                android:width="2dp"
                android:color="@color/suai_secondary"/>
        </shape>
    </item>

    <item
        android:id="@+id/itemTableBorderRight"
        android:left="-2dp"
        android:bottom="-2dp"
        android:top="-2dp">
        <shape android:id="@+id/itemTableBorderRightShape" android:shape="rectangle">
            <stroke
                android:id="@+id/itemTableBorderRightStroke"
                android:width="2dp"
                android:color="@color/suai_secondary"/>
        </shape>
    </item>
</layer-list>